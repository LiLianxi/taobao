<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taobao.order.mapper.OrderMapper">

    <!-- query order by id -->
    <select id="queryOrderById" resultType="java.lang.Integer">
        select * from tb_order where id = #{id}
    </select>
</mapper>